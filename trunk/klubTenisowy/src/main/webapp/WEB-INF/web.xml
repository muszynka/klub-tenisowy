<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name>klubTenisowy2</display-name>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>KlubTenisowy</servlet-name>
		<servlet-class>com.agh.klubTenisowy.KlubTenisowy</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>KlubTenisowy</servlet-name>
		<url-pattern>/KlubTenisowy</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>KlubTenisowyAdmin</servlet-name>
		<servlet-class>com.agh.klubTenisowy.KlubTenisowyAdmin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>KlubTenisowyAdmin</servlet-name>
		<url-pattern>/KlubTenisowyAdmin</url-pattern>
	</servlet-mapping>
	
	
	
	

	

</web-app>
